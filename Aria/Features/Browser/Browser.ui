<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.99.8 -->
<interface>
    <!-- interface-name MainWindow.ui -->
    <requires lib="gtk" version="4.0"/>
    <requires lib="libadwaita" version="1.4"/>
    <template class="AriaFeaturesBrowserBrowser" parent="GtkBox">
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <child>
            <object class="GtkStack" id="browser-stack">
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <child>
                    <object class="GtkStackPage" id="empty-collection-stack-page">
                        <property name="child">
                            <object class="AriaFeaturesBrowserEmptyCollectionPage" id="empty-collection-page"/>
                        </property>
                        <property name="name">empty-collection-stack-page</property>
                    </object>
                </child>
                <child>
                    <object class="GtkStackPage" id="browser-stack-page">
                        <property name="child">
                            <object class="AdwNavigationView" id="main-navigation">
                                <child>
                                    <object class="AdwNavigationPage" id="collection-navigation-page">
                                        <property name="tag">collection-page</property>
                                        <property name="title">Collection</property>
                                        <child>
                                            <object class="AdwBreakpointBin">
                                                <property name="child">
                                                    <object class="AdwNavigationSplitView">
                                                        <property name="content">
                                                            <object class="AdwNavigationPage">
                                                                <child>
                                                                    <object class="AriaFeaturesBrowserArtistArtistPage"
                                                                            id="browser-artist-page"/>
                                                                </child>
                                                            </object>
                                                        </property>
                                                        <property name="sidebar">
                                                            <object class="AriaFeaturesBrowserArtistsArtistsPage"
                                                                    id="artists-page"/>
                                                        </property>
                                                    </object>
                                                </property>
                                                <property name="height-request">200</property>
                                                <property name="width-request">320</property>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                                <child>
                                    <object class="AdwNavigationPage" id="search-navigation-page">
                                        <property name="tag">search-page</property>
                                        <property name="title">Search</property>
                                    </object>
                                </child>
                            </object>
                        </property>
                        <property name="name">browser-stack-page</property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
