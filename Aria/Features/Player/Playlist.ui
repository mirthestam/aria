<interface>
    <!-- interface-name PlaylistBox.ui -->
    <requires lib="gtk" version="4.0"/>
    <requires lib="libadwaita" version="1.4"/>
    <template class="AriaFeaturesPlayerPlaylist" parent="GtkStack">
        <property name="vexpand">True</property>
        <property name="vhomogeneous">False</property>
        <child>
            <object class="GtkStackPage" id="empty-playlist-page">
                <property name="child">
                    <object class="AdwStatusPage">
                        <property name="css-classes">compact
                            no-drop-highlight
                        </property>
                        <property name="icon-name">documentinfo-symbolic</property>
                        <property name="title">Playlist is Empty</property>
                    </object>
                </property>
                <property name="name">empty-playlist-page</property>
            </object>
        </child>
        <child>
            <object class="GtkStackPage" id="playlist-page">
                <property name="child">
                    <object class="GtkOverlay" id="now-playing-overlay">
                        <property name="child">
                            <object class="GtkScrolledWindow">
                                <property name="propagate-natural-height">True</property>
                            </object>
                        </property>
                    </object>
                </property>
                <property name="name">playlist</property>
            </object>
        </child>
    </template>
</interface>
