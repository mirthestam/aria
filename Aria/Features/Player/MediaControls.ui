<interface>
    <!-- interface-name PlaylistBox.ui -->
    <requires lib="gtk" version="4.0"/>
    <requires lib="libadwaita" version="1.4"/>
    <template class="AriaFeaturesPlayerMediaControls" parent="GtkBox">
        <property name="halign">center</property>
        <property name="hexpand">False</property>
        <property name="spacing">6</property>
        <property name="vexpand">True</property>
        <child>
            <object class="GtkButton" id="skip-backward-button">
                <property name="action-name">player.prev</property>
                <property name="icon-name">media-skip-backward-symbolic</property>
            </object>
        </child>
        <child>
            <object class="GtkButton" id="playback-start-button">
                <property name="icon-name">media-playback-start-symbolic</property>
            </object>
        </child>
        <child>
            <object class="GtkButton" id="skip-forward-button">
                <property name="action-name">player.next</property>
                <property name="icon-name">media-skip-forward-symbolic</property>
            </object>
        </child>
    </template>
</interface>
