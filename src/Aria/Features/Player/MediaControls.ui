<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.99.9 -->
<interface>
  <!-- interface-name PlaylistBox.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="AriaFeaturesPlayerMediaControls" parent="GtkBox">
    <property name="halign">center</property>
    <property name="hexpand">False</property>
    <property name="spacing">6</property>
    <property name="vexpand">True</property>
    <child>
      <object class="GtkButton" id="skip-backward-button">
        <property name="action-name">aria-player.previous</property>
        <property name="icon-name">media-skip-backward-symbolic</property>
        <property name="tooltip-text">Previous track</property>
      </object>
    </child>
    <child>
      <object class="AriaFeaturesSharedPlayButton" id="playback-start-button">
        <property name="action-name">aria-player.play-pause</property>
        <property name="icon-name">media-playback-start-symbolic</property>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="skip-forward-button">
        <property name="action-name">aria-player.next</property>
        <property name="icon-name">media-skip-forward-symbolic</property>
        <property name="tooltip-text">Next track</property>
      </object>
    </child>
  </template>
</interface>
