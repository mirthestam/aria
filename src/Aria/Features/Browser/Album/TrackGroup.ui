<interface>
  <!-- interface-name MainWindow.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="AriaFeaturesBrowserAlbumTrackGroup" parent="GtkBox">
    <property name="orientation">vertical</property>
    <property name="spacing">6</property>
    <child>
      <object class="GtkBox" id="header-box">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkLabel" id="header-label">
                <property name="css-classes">heading</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="info-box">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkGrid">
                <property name="valign">fill</property>
                <child>
                  <object class="GtkLabel" id="composers-label">
                    <property name="css-classes">dimmed</property>
                    <property name="label">Composed by</property>
                    <property name="margin-bottom">3</property>
                    <property name="margin-top">3</property>
                    <property name="valign">start</property>
                    <property name="xalign">1.0</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="AdwWrapBox" id="composers-box">
                    <property name="child-spacing">0</property>
                    <property name="halign">start</property>
                    <property name="margin-start">0</property>
                    <property name="margin-top">0</property>
                    <property name="valign">fill</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="conductors-label">
                    <property name="css-classes">dimmed</property>
                    <property name="label">Conducted by</property>
                    <property name="margin-bottom">3</property>
                    <property name="margin-top">3</property>
                    <property name="valign">start</property>
                    <property name="xalign">1.0</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="AdwWrapBox" id="conductors-box">
                    <property name="child-spacing">0</property>
                    <property name="halign">start</property>
                    <property name="margin-top">0</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="arranged-label">
                    <property name="css-classes">dimmed</property>
                    <property name="label">Arranged by</property>
                    <property name="margin-end">3</property>
                    <property name="margin-top">3</property>
                    <property name="valign">start</property>
                    <property name="xalign">1.0</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="AdwWrapBox" id="arranged-box">
                    <property name="child-spacing">0</property>
                    <property name="halign">start</property>
                    <property name="margin-start">0</property>
                    <property name="margin-top">0</property>
                    <property name="valign">fill</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="performers-label">
                    <property name="css-classes">dimmed</property>
                    <property name="label">Performed by</property>
                    <property name="margin-bottom">3</property>
                    <property name="margin-top">3</property>
                    <property name="valign">start</property>
                    <property name="xalign">1.0</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="AdwWrapBox" id="performers-box">
                    <property name="child-spacing">0</property>
                    <property name="halign">start</property>
                    <property name="margin-start">0</property>
                    <property name="margin-top">0</property>
                    <property name="valign">fill</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkListBox" id="tracks-listbox">
        <property name="css-classes">boxed-list</property>
      </object>
    </child>
  </template>
</interface>
